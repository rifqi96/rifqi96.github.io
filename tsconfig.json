{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "include": [
    "**/*.vue",
    "**/*.ts",
    "*.d.ts",
    "./.eslintrc.cjs",
    "./.nuxt/*.ts",
    "./.nuxt/**/*.ts",
    "./.nuxt/*.js",
    "./.nuxt/**/*.js",
    "./.nuxt/*.vue",
    "./.nuxt/**/*.vue",
    "./.nuxt/*.d.ts",
    "./.nuxt/**/*.d.ts",
    "./.nuxt/*.json",
    // "auto-imports.d.ts",
    "components.d.ts",
    "types/**/*.d.ts",
    "domains/**/*.vue",
    "domains/**/*.ts",
    "layouts/**/*.vue",
    "pages/**/*.vue",
    "composables/**/*.ts",
    "server/**/*.ts",
    "utils/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "public",
    "dist",
    "dist/**/*",
    "coverage",
    "output",
    "eslintrc.cjs",
    "public/margin-calc-legacy",
    "public/margin-calc-legacy/**/*"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "declaration": false,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "bundler",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"],
      "@domains/*": ["./domains/*"],
      "types": ["vuetify", "vite/client"],
      "#imports": ["./.nuxt/imports"],
      "#build/*": ["./.nuxt/*"],
      "#app": ["./node_modules/nuxt/dist/app"],
      "#app/*": ["./node_modules/nuxt/dist/app/*"],
      "#image": ["./node_modules/@nuxt/image/dist/runtime"],
      "#image/*": ["./node_modules/@nuxt/image/dist/runtime/*"]
    },
    "types": ["node", "vite/client", "vuetify", "@nuxt/types"],
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "jsx": "preserve",
    "allowJs": true,
    "ignoreDeprecations": "5.0"
  },
  "references": []
}
