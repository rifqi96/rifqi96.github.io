{
  "name": "margin-calc-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "yarn typecheck && yarn lint:prettierWrite && yarn lint:js --fix",
    "lint:prettierWrite": "prettier --write --list-different .",
    "lint:js": "eslint --ext \".vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts\" --ignore-path .gitignore --ignore-pattern \"margin-calc/**/*\" .",
    "lint:prettier": "prettier --check .",
    "lint:preview": "yarn lint:js && yarn lint:prettier",
    "typecheck": "vue-tsc --emitDeclarationOnly --declaration --skipLibCheck",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^20.10.4",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "yarn lint",
    "*.yml": "yarn lint:prettierWrite"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "*",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  }
}
